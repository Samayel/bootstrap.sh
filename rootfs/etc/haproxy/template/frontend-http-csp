    http-response set-header Content-Security-Policy                "upgrade-insecure-requests; report-uri https://quyo.report-uri.com/r/default/csp/enforce"
    http-response set-header Content-Security-Policy-Report-Only    "upgrade-insecure-requests; default-src https: wss: data: blob:; script-src 'self' 'unsafe-inline' 'unsafe-eval'; style-src 'self' 'unsafe-inline'; img-src 'self' data:; report-uri https://quyo.report-uri.com/r/default/csp/reportOnly"
